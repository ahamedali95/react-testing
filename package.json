{
  "name": "react-testing",
  "version": "1.0.0",
  "description": "React Testing",
  "main": "index.js",
  "jest": {
    "setupFiles": [
      "<rootDir>/enzyme.config.js"
    ],
    "testPathIgnorePatterns": [
      "<rootDir>/__tests__/testUtils/testHelpers.js"
    ]
  },
  "scripts": {
    "start": "webpack-dev-server --progress --config config/webpack.dev.js",
    "build:dev": "webpack --progress --config config/webpack.dev.js",
    "build:prod": "webpack --progress --config config/webpack.prod.js",
    "watch": "jest --watch",
    "jest-clear-cache": "jest --clearCache",
    "coverage": "jest --coverage"
  },
  "dependencies": {
    "@babel/core": "^7.6.0",
    "@babel/preset-env": "^7.6.0",
    "@babel/preset-react": "^7.0.0",
    "babel-jest": "^24.9.0",
    "babel-loader": "^8.0.6",
    "bootstrap": "^4.3.1",
    "clean-webpack-plugin": "^1.0.1",
    "copy-webpack-plugin": "^5.0.3",
    "css-loader": "^2.1.1",
    "enzyme": "^3.10.0",
    "enzyme-adapter-react-16": "^1.14.0",
    "file-loader": "^4.0.0",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "^3.2.0",
    "jest": "^24.9.0",
    "mini-css-extract-plugin": "^0.7.0",
    "moment": "^2.24.0",
    "optimize-css-assets-webpack-plugin": "^5.0.1",
    "raf": "^3.4.1",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-hot-loader": "^4.9.0",
    "style-loader": "^0.23.1",
    "uuid": "^3.3.3",
    "webpack": "^4.33.0",
    "webpack-bundle-analyzer": "^3.3.2",
    "webpack-cli": "^3.3.3",
    "webpack-dev-server": "^3.7.1",
    "webpack-merge": "^4.2.1",
    "whatwg-fetch": "^3.0.0"
  }
}
